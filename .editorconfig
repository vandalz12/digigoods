root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
indent_style = space
indent_size = 2

[*.java]
indent_size = 2

[*.{xml,pom}]
indent_size = 2

[*.properties]
indent_size = 2

[*.{md,txt}]
trim_trailing_whitespace = false

[*.{cmd,bat}]
end_of_line = crlf